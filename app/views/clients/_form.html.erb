<div class="modal-dialog">
  <div class="modal-content">
    <%= simple_form_for @client, remote: true, html: { style: "display:inline;" } do |f| %>
      <div class="modal-body">
        <ul class="errors"></ul>

      <div class="form-group">
        <%= f.label :billing_contact, class:"control-label" %>
        <%= f.text_field :billing_contact, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :billing_company, class:"control-label" %>
        <%= f.text_field :billing_company, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :Billing_Address, class:"control-label" %>
        <%= f.text_field :billing_address1, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :Suite, class:"control-label" %>
        <%= f.text_field :billing_address2, class: "form-control" %>
        <%= f.label :city, class:"control-label" %>
        <%= f.text_field :billing_city, class: "form-control" %>
      </div>
      <div class="form-group">

      </div>

      <div class="form-group">
        <%= f.label :state, class:"control-label" %>
        <%= f.text_field :billing_state, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :zip, class:"control-label" %>
        <%= f.text_field :billing_zip, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :billing_phone, class:"control-label" %>
        <%= f.text_field :billing_phone, class: "form-control" %>
      </div>


      <div class="modal-footer">
        <%= f.submit class: "btn btn-primary" %>
        <%= link_to "Cancel", "#", class: "btn", data: {dismiss: "modal"} %>
      </div>
          </div>
    <% end %>
  </div>
</div>
