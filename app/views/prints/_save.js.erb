$("ul.errors").html("")
<% if @print.errors.any? %>
  <% @print.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".print-index").html("<%= escape_javascript(render 'index') %>")
  $("#print-modal").modal("hide")
<% end %>
